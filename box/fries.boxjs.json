        }
      ],
      "author": "@VirgilClyne",
      "repo": "https://github.com/VirgilClyne/GetSomeFries/blob/beta/js/Disney2B.js",
      "script": "https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Disney2B.js",
      "icons": ["https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Disney+.png", "https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Disney+.png"]
    }
  ]
