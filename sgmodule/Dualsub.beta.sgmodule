#!name=Dualsub
#!desc=(BETA) Disney+, Star+, HBO Max, Netflix, YouTube subtitles add-ons

[Script]
Dualsub.Disney_Plus.m3u8 = type=http-response,pattern=^https?:\/\/(.*)\.media\.(dssott|starott)\.com\/ps01\/disney\/([0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12})\/(.*)-all-(.*)\.m3u8(.*),requires-body=1,max-size=0,timeout=30,script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Dualsub.beta.js
Dualsub.Disney_Plus.vtt = type=http-response,pattern=^https?:\/\/(.*)\.media\.(dssott|starott)\.com\/ps01\/disney\/([0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12})\/(.*)\.vtt$,requires-body=1,max-size=0,timeout=30,script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Dualsub.beta.js
Dualsub.HBO_Max.m3u8 = type=http-response,pattern=^https?:\/\/((.*)\.hbomaxcdn|manifests\.v2\.api\.hbo)\.com\/hls\.m3u8(.*),requires-body=1,max-size=0,timeout=30,script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Dualsub.beta.js
Dualsub.HBO_Max.vtt = type=http-response,pattern=^https?:\/\/((.*)\.hbomaxcdn|manifests\.v2\.api\.hbo)\.com\/video(.*)\.vtt$,requires-body=1,max-size=0,timeout=30,script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Dualsub.beta.js
Dualsub.Netflix = type=http-response,pattern=^https?:\/\/(.*)\.nflxvideo\.net\/\?o=(\d+)&v=(\d+)&e=(.*),requires-body=1,max-size=0,timeout=30,script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Dualsub.beta.js
Dualsub.NetflixParamount_Plus= type=http-response,pattern=^https?:\/\/(.*)\.cbs(aa|i)video\.com\/(.*)\.vtt(\?m=\d+)?,requires-body=1,max-size=0,timeout=30,script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Dualsub.beta.js
Dualsub.YouTube = type=http-request,pattern=^https?:\/\/www\.youtube\.com\/api\/timedtext(.*),requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Dualsub.beta.js

[MITM]
hostname = %APPEND% *.media.dssott.com, *.media.starott.com, *.api.hbo.com, *.hbomaxcdn.com, *.nflxvideo.net, www.youtube.com
