#!name=🍟 Cloudflare DDNS
#!desc=Auto Update Cloudflare DDNS
#!system=mac

[Rule]
DOMAIN-SUFFIX,my-ip.io,DIRECT

[Script]
Cloudflare DDNS = type=event,network-changed,argument=Token=令牌&zone_id=区域ID&dns_records_id=记录ID&dns_records_name=记录名称&dns_records_proxied=是否代理,script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/main/js/Cloudflare_DDNS.js
Cloudflare DDNS = type=cron,cronexp="*/10 * * * *",argument=Token=令牌&zone_id=区域ID&dns_records_id=记录ID&dns_records_name=记录名称&dns_records_proxied=是否代理,script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/main/js/Cloudflare_DDNS.js
