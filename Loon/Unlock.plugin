#!name=APP功能
#!desc=解锁订阅，高级功能
#!icon= https://raw.githubusercontent.com/iEwha/Profiles/master/icon/Unlock.png
[Script]
# > VSCO 解锁高级特权
http-response pattern=^https?:\/\/api\.revenuecat\.com\/v\d\/subscribers\/ script-path=https://raw.githubusercontent.com/NobyDa/Script/master/QuantumultX/File/vsco.js, requires-body=true, timeout=10, tag=vsco
# > PS Express解锁高级订阅
http-response pattern=^https:\/\/lcs-mobile-cops\.adobe\.io\/mobile_profile  requires-body=true, script-path=https://raw.githubusercontent.com/iEwha/Profiles/master/Script/photoshop.js,tag=PS Express
# > Emby解锁
http-response pattern=^https?:\/\/mb3admin.com\/admin\/service\/registration\/validateDevice requires-body=true, script-path=https://raw.githubusercontent.com/iEwha/Profiles/master/Surge/Scripts/EmbyPremiere.js, tag=Emby
# > Adguardpremium
http-response pattern=^https:\/\/mobile-api\.adguard\.com\/api\/1\.0\/ios_validate_receipt$ requires-body=true, script-path=https://raw.githubusercontent.com/iEwha/Profiles/master/Script/adguard.js, tag=Adguard
# > 西窗烛 登录使用解锁终身会员 
http-response pattern=https://lchttpapi.xczim.com/*  requires-body=true, script-path=https://raw.githubusercontent.com/iEwha/Profiles/master/Script/xcz.js, tag=西窗烛
# > 小影 解锁高级功能
http-response pattern=^https:\/\/api-chn\.rthdo\.com\/api\/rest\/u\/vipVerifyReceipt requires-body=true, script-path= https://raw.githubusercontent.com/iEwha/Profiles/master/Script/vivavideo.js, tag=小影
# > NiChi 解锁素材
http-response pattern=https?:\/\/m(p|ini-hk)\.bybutter\.com\/mood\/(official-templates|privileges) requires-body=true, script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/NiChi.js, tag=NiChi
# > Polarr泼辣修图 解锁特权 (需恢复购买)
http-response pattern=^https:\/\/api\.polaxiong\.com\/v1\/payments\/appleiap\/receipts\/confirmation requires-body=true, script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/Polarr.js, tag=泼辣修图
# > Spotify 部分解锁premium 
http-response pattern=^https:\/\/spclient\.wg\.spotify\.com\/(bootstrap\/v1\/bootstrap|user-customization-service\/v1\/customize)$,requires-body=true,binary-body-mode=true script-path=https://raw.githubusercontent.com/app2smile/rules/master/js/spotify-proto.js, tag=Spotify
# > 白描黄金会员
http-response pattern=^https?:\/\/baimiao\.uzero\.cn\/api\/v\d\.user\/appLaunchWithUser$,requires-body=true, script-path=https://raw.githubusercontent.com/iEwha/Profiles/master/Script/baimiao.js, tag=白描
# > 美图
http-response pattern=^https?://(api|h5).xiuxiu.meitu.com/(v1/user/show.json|v1/vip/vip_show.json|v1/vip/prompt/query.json|v1/h5/vip/sub_detail.json|v1/h5/user/self_show.json|v1/h5/vip/user_detail.json|v1/vip/prompt/query.json|v1/vip/prompt/query.json), requires-body=true, script-path=https://raw.githubusercontent.com/iEwha/Profiles/master/Script/mtxx.js, tag=美图秀秀

