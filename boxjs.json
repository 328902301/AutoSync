{
    "id": "kokoryh.app.sub",
    "name": "kokoryh 脚本订阅",
    "description": "该订阅兼容BoxJs以及QX Gallery",
    "author": "@kokoryh",
    "icon": "https://avatars.githubusercontent.com/u/75901403",
    "repo": "https://github.com/kokoryh/Script/tree/master",
    "apps": [{
      "id": "MGClub",
      "name": "毛怪俱乐部",
      "keys": ["Cookie_MGClub"],
      "descs_html": [
        "<h4 align=\"center\">获取Cookie以及脚本配置请查看<a href=\"https://raw.githubusercontent.com/kokoryh/Script/master/js/ihan.js\">脚本注释</a></h4>"
      ],
      "script": "https://raw.githubusercontent.com/kokoryh/Script/master/js/ihan.js",
      "author": "@kokoryh",
      "repo": "https://github.com/kokoryh/Script/blob/master/js/ihan.js",
      "icons": ["https://raw.githubusercontent.com/kokoryh/Script/master/images/mgclub.png", "https://raw.githubusercontent.com/kokoryh/Script/master/images/mgclub.png"]
    }, {
      "id": "BiliSuit",
      "name": "B站装扮监控",
      "keys": [
        "bili_suit_list"
      ],
      "author": "@kokoryh",
      "repo": "https://github.com/kokoryh/Script/blob/master/js/bili-suit.js",
      "icons": [
        "https://raw.githubusercontent.com/NobyDa/mini/master/Alpha/bilibili.png",
        "https://raw.githubusercontent.com/NobyDa/mini/master/Color/bilibili.png"
      ],
      "script": "https://raw.githubusercontent.com/kokoryh/Script/master/js/bili-suit.js",
      "settings": [
        {
          "id": "bili_suit_list",
          "name": "装扮列表",
          "val": "",
          "type": "textarea",
          "placeholder": "推荐由脚本自动获取装扮信息，如有特殊情况可手动填写，必须为Json格式",
          "autoGrow": true,
          "desc": "多个装扮信息，如出现异常，请删除全部内容，避免直接编辑数据"
        }
      ]
    }, {
      "id": "BiliSuitDIY",
      "name": "装扮DIY",
      "keys": [
        "bili_suit_push",
        "bili_no_load",
        "bili_suit_view",
        "@bili_suit.diy",
        "bili_user_equip",
        "bili_load_equip"
      ],
      "author": "@kokoryh",
      "repo": "https://github.com/kokoryh/Script/blob/master/js/bili-suit-diy.js",
      "script": "https://raw.githubusercontent.com/kokoryh/Script/master/js/bili-suit-detail.js",
      "icons": [
        "https://raw.githubusercontent.com/NobyDa/mini/master/Alpha/bilibili.png",
        "https://raw.githubusercontent.com/NobyDa/mini/master/Color/bilibili.png"
      ],
      "settings": [
        {
          "id": "bili_suit_push",
          "name": "装扮追加",
          "val": false,
          "type": "boolean",
          "desc": "此选项为true时新的装扮信息将追加于原本的数据之后，为false时会覆盖原本的数据。若开启了『不提取加载动画』，则仅追加主题"
        },
        {
          "id": "bili_no_load",
          "name": "不提取加载动画",
          "val": false,
          "type": "boolean",
          "desc": "此选项为true时不提取加载动画"
        },
        {
          "id": "@bili_suit.diy.skip_pink",
          "name": "对粉色B站不生效",
          "val": false,
          "type": "boolean",
          "desc": "此选项为true时脚本对粉色B站不生效"
        },
        {
          "id": "bili_suit_view",
          "name": "装扮总览",
          "val": "",
          "type": "textarea",
          "placeholder": "",
          "autoGrow": true,
          "desc": "本栏仅供浏览，无实际意义"
        },
        {
          "id": "@bili_suit.diy.skin_num",
          "name": "使用第几套主题",
          "val": "1",
          "type": "text",
          "placeholder": "",
          "desc": "针对有多套主题的装扮，默认为1，只允许输入数字"
        },
        {
          "id": "@bili_suit.diy.load_num",
          "name": "使用第几个加载动画",
          "val": "1",
          "type": "text",
          "placeholder": "",
          "desc": "针对有多个加载动画的装扮，默认为1，只允许输入数字"
        },
        {
          "id": "bili_user_equip",
          "name": "主题信息",
          "val": "",
          "type": "textarea",
          "rows": 2,
          "placeholder": "",
          "desc": ""
        },
        {
          "id": "bili_load_equip",
          "name": "加载动画信息",
          "val": "",
          "type": "textarea",
          "rows": 2,
          "placeholder": "",
          "desc": "下拉出现的加载动画"
        }
      ]
    }]
}
