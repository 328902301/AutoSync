      ],
      "author": "@VirgilClyne",
      "icons": [
        "https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Cloudflare.png",
        "https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Cloudflare.png"
      ]
    }
  ]
