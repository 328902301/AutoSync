name: IP 网络信息 Tile 面板
desc: 包含国内和国外. 自己设置对应域名的分流

tiles:
  - name: "网络信息"
    interval: 300
    title: "网络信息"
    content: "..."
    argument: "icon=antenna.radiowaves.left.and.right.circle.fill&backgroundColor=#178BD8"
    # argument: "icon=antenna.radiowaves.left.and.right.circle.fill&color=#5d84f8"
    # icon: "arrow.up.arrow.down.circle"
    # backgroundColor: "#663399"

script-providers:
  "网络信息":
    url: "https://raw.githubusercontent.com/xream/scripts/main/surge/modules/network-info/index.js"
    interval: 86400
