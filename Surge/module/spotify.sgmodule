# 仓库地址 https://github.com/app2smile/rules
# 搜索界面 歌单/歌曲可以随意切换完全播放
# 音乐库已点赞歌曲随意切换完全播放

[Script]
spotify-proto = type=http-response,pattern=^https:\/\/spclient\.wg\.spotify\.com\/(bootstrap\/v1\/bootstrap|user-customization-service\/v1\/customize)$,requires-body=1,binary-body-mode=1,max-size=0,script-path=https://gitlab.com/kokoryh/script/-/raw/master/js/spotify-proto.js,script-update-interval=0

[MITM]
hostname = %INSERT% spclient.wg.spotify.com
