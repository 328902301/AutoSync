{
    "id": "fokit.app.sub",
    "name": "FoKit 自用订阅",
    "author": "@FoKit",
    "icon": "https://avatars.githubusercontent.com/u/44187836",
    "repo": "https://github.com/FoKit",
    "apps": [
        {
            "id": "ql",
            "name": "数据同步",
            "author": "@FoKit",
            "descs_html": [
                "用于 JD1 / JD2 和 BoxJS 之间的数据同步"
            ],
            "icons": [
                "https://raw.githubusercontent.com/FoKit/Quantumult-X/main/images/sync.png",
                "https://raw.githubusercontent.com/FoKit/Quantumult-X/main/images/sync.png"
            ],
            "keys": [
                "@ql.ip_jd1",
                "@ql.client_id_jd1",
                "@ql.client_secret_jd1",
                "@ql.username_jd1",
                "@ql.password_jd1",
                "@ql.env_jd1",
                "@ql.mute_jd1",
                "@ql.is_pwd_jd1",
                "@ql.ip_jd2",
                "@ql.client_id_jd2",
                "@ql.client_secret_jd2",
                "@ql.username_jd2",
                "@ql.password_jd2",
                "@ql.env_jd2",
                "@ql.mute_jd2",
                "@ql.is_pwd_jd2"
            ],
            "settings": [
                {
                    "id": "@ql.mute_jd1",
                    "name": "静默同步（JD1 > BoxJs）",
                    "val": false,
                    "type": "boolean",
                    "desc": "同步时不会显示通知"
                },
                {
                    "id": "@ql.is_pwd_jd1",
                    "name": "使用密码登陆 JD1 容器",
                    "val": false,
                    "type": "boolean",
                    "desc": "优先使用密码登陆"
                },
                {
                    "id": "@ql.ip_jd1",
                    "name": "JD1 Domains",
                    "val": "",
                    "type": "text",
                    "placeholder": "ip:port"
                },
                {
                    "id": "@ql.username_jd1",
                    "name": "JD1 UserName",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.password_jd1",
                    "name": "JD1 PassWord",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.client_id_jd1",
                    "name": "JD1 Client ID",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.client_secret_jd1",
                    "name": "JD1 Client Secret",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.mute_jd2",
                    "name": "静默同步（JD2 > BoxJs）",
                    "val": false,
                    "type": "boolean",
                    "desc": "同步时不会显示通知"
                },
                {
                    "id": "@ql.is_pwd_jd2",
                    "name": "使用密码登陆",
                    "val": true,
                    "type": "boolean",
                    "desc": "优先使用密码登陆"
                },
                {
                    "id": "@ql.ip_jd2",
                    "name": "JD2 Domains",
                    "val": "",
                    "type": "text",
                    "placeholder": "ip:port"
                },
                {
                    "id": "@ql.username_jd2",
                    "name": "JD2 UserName",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.password_jd2",
                    "name": "JD2 PassWord",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.client_id_jd2",
                    "name": "JD2 Client ID",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.client_secret_jd2",
                    "name": "JD2 Client Secret",
                    "val": "",
                    "type": "text"
                }
            ],
            "scripts": [
                {
                    "name": "JD1 同步到 BoxJS",
                    "script": "https://raw.githubusercontent.com/FoKit/Quantumult-X/main/scripts/jd1_sync_box.js"
                },
                {
                    "name": "JD2 同步到 BoxJS",
                    "script": "https://raw.githubusercontent.com/FoKit/Quantumult-X/main/scripts/jd2_sync_box.js"
                }
            ]
        },
        {
            "id": "wskey",
            "name": "京东 WsKey",
            "author": "@FoKit",
            "descs_html": [
                "用于自动抓取京东 WsKey"
            ],
            "icons": [
                "https://raw.githubusercontent.com/FoKit/Quantumult-X/main/images/jdjoy.png",
                "https://raw.githubusercontent.com/FoKit/Quantumult-X/main/images/jdjoy.png"
            ],
            "keys": [
                "WSKEY_TG_BOT_TOKEN",
                "WSKEY_TG_USER_ID",
                "wskeyList"
            ],
            "settings": [
                {
                    "id": "WSKEY_TG_BOT_TOKEN",
                    "name": "Telegram 机器人 Token",
                    "val": "",
                    "type": "text",
                    "desc": "填写 Telegram 机器人的 Token，格式：123456:abcdefgabcdefgabcdefg"
                },
                {
                    "id": "WSKEY_TG_USER_ID",
                    "name": "Telegram会话ID",
                    "val": "[\"-1001701121614\"]",
                    "type": "text",
                    "desc": "填写 Telegram 会话 ID，以数组形式保存[\"-1001701121614\"]"
                },
                {
                    "id": "wskeyList",
                    "name": "京东 WsKey",
                    "val": "",
                    "type": "text",
                    "desc": "京东 WsKey 列表"
                }
            ]
        }
    ]
}