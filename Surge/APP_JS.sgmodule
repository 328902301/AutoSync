hostname = %APPEND% ios-*.prod.ftl.netflix.com, ios.prod.ftl.netflix.com, guide-acs.m.taobao.com, testflight.apple.com, mb3admin.com, mp.weixin.qq.com, api.revenuecat.com, ap*.intsig.net, *.baidu.com, www.zhihu.com, duckduckgo.com, biz.caiyunapp.com, wmapi.meituan.com, hd.mina.mi.com
