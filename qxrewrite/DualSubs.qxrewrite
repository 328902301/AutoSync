#!name= DualSubs
#!desc= (0.2.0) Dual Subtitiles for Streaming Media Platform

#[Script]
# DualSubs.YouTube
^https?:\/\/www\.youtube\.com\/api\/timedtext(.*) url script-request-header https://raw.githubusercontent.com/DualSubs/DualSubs/main/js/DualSubs.js
# DualSubs.Disney_Plus.m3u8
^https?:\/\/(.*)\.media\.(dssott|starott)\.com\/ps01\/disney\/([0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12})\/(.*)-all-(.*)\.m3u8(.*) url script-response-body https://raw.githubusercontent.com/DualSubs/DualSubs/main/js/DualSubs.js
# DualSubs.Disney_Plus.vtt
^https?:\/\/(.*)\.media\.(dssott|starott)\.com\/ps01\/disney\/([0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12})\/(.*)\.vtt$ url script-response-body https://raw.githubusercontent.com/DualSubs/DualSubs/main/js/DualSubs.js
# DualSubs.Prime_video.m3u8
^https?:\/\/(.*)\.(hls\.row\.aiv-cdn|akamaihd|cloudfront)\.net\/(.*)\/([0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12})\.m3u8$ url script-response-body https://raw.githubusercontent.com/DualSubs/DualSubs/main/js/DualSubs.js
# DualSubs.Prime_video.vtt
^https?:\/\/(.*)\.cloudfront\.net\/(.*)\/([0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12})\.vtt$ url script-response-body https://raw.githubusercontent.com/DualSubs/DualSubs/main/js/DualSubs.js
# DualSubs.HBO_Max.m3u8
^https?:\/\/((.*)\.hbomaxcdn|manifests\.v2\.api\.hbo)\.com\/hls\.m3u8(.*) url script-response-body https://raw.githubusercontent.com/DualSubs/DualSubs/main/js/DualSubs.js
# DualSubs.HBO_Max.vtt
^https?:\/\/((.*)\.hbomaxcdn|manifests\.v2\.api\.hbo)\.com\/video(.*)\.vtt$ url script-response-body https://raw.githubusercontent.com/DualSubs/DualSubs/main/js/DualSubs.js
# DualSubs.Paramount_Plus.vtt
^https?:\/\/(.*)\.cbs(aa|i)video\.com\/(.*)\.vtt(\?m=\d+)? url script-response-body https://raw.githubusercontent.com/DualSubs/DualSubs/main/js/DualSubs.js
# DualSubs.Netflix
^https?:\/\/(.*)\.nflxvideo\.net\/\?o=(\d+)&v=(\d+)&e=(.*) url script-response-body https://raw.githubusercontent.com/DualSubs/DualSubs/main/js/DualSubs.js

#[Map Local]
#^https?:\/\/raw\.githubusercontent\.com\/DualSubs\/DualSubs\/main\/database\/DualSubs\.jsonc data="https://raw.githubusercontent.com/DualSubs/DualSubs/main/database/DualSubs.jsonc"

