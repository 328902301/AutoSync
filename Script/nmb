<script src="js/mui.min.js"></script>
<script>

    function clear_cache() {

            $.clear_cache();

    }
 function logout(){

        $.action_sheet("注销当前账号,直接关闭应用",function (res) {
            if(res==1){
                $.logout();
                var url = "/index.php?m=Home&c=Index&a=my";
                window.location.href=url;
            }else{
                $.exit();
            }
        })

 }

 function relogin(){
     $.open("/index.php?m=Home&c=Index&a=login");
 }
  
  function share() {

     if(window['Android']){
         var api = window['Android'];
         api.do_share();
     }
     else {
         window.webkit.messageHandlers.AppModel.postMessage({"method":"do_share","params":'',"callback":""})
     }
 }

 function  feedBack() {
     if(window['Android']){
         var api = window['Android'];
         api.feedback();
     }
     else {
         window.webkit.messageHandlers.AppModel.postMessage({"method":"feedback","params":'',"callback":""})
     }

 }
 function memberPrepaid(url) {
     if(window['Android']){
         var api = window['Android'];
         api.bannerClick(url);
     }
     else {
         window.webkit.messageHandlers.AppModel.postMessage({"method":"bannerClick","params":url,"callback":""})
     }
 }
</script>
