{
  "id": "Obsidian.app.sub",
  "name": "Obsidian Task_Gallery",
  "description": "该订阅兼容BoxJs以及QX Gallery",
  "author": "@Obsidian",
  "icon": "https://avatars.githubusercontent.com/u/10055901",
  "repo": "https://github.com/Obsidian99/Profile/tree/master",
  "apps": [{
      "id": "NeteaseMusic",
      "name": "网易云音乐",
      "keys": ["chavy_cookie_neteasemusic"],
      "settings": [
        { "id": "CFG_neteasemusic_retryCnt", "name": "重试次数", "val": 10, "type": "text", "desc": "一直尝试签到直至出现“重复签到”标识!" },
        {
          "id": "CFG_neteasemusic_retryInterval",
          "name": "重试间隔 (毫秒)",
          "val": 500,
          "type": "text",
          "desc": "每次重试间隔时间 (毫秒)！"
        }
      ],
      "author": "@chavyleung",
      "repo": "https://github.com/chavyleung/scripts/tree/master/neteasemusic",
      "script": "https://raw.githubusercontent.com/chavyleung/scripts/master/neteasemusic/neteasemusic.js",
      "icons": [
        "https://raw.githubusercontent.com/Orz-3/mini/master/Alpha/neteasemusic.png",
        "https://raw.githubusercontent.com/Orz-3/mini/master/Color/neteasemusic.png"]
     },
    {
      "id": "zZ.umetrip",
      "name": "航旅纵横(pro)",
      "desc_html": "请先按<a href=\"https://raw.githubusercontent.com/zZPiglet/Task/master/umetrip/umetrip.js\">脚本注释</a>进行配置，然后进入\"航旅纵横(Pro)\" - \"出行有礼\" - \"每日签到\"，若弹出\"首次写入 umetrip id 成功\"即可正常食用。",
      "keys": ["@umetrip.id"],
      "settings": [
        {
          "id": "@umetrip.id",
          "name": "rsid",
          "val": "",
          "type": "text",
          "autoGrow": true,
          "rows": 2,
          "desc": "用户验证信息。若抓包填入请填写 rsid 中「$$」之前的部分。"
        }
      ],
      "author": "@zZPiglet",
      "repo": "https://github.com/zZPiglet/Task/blob/master/umetrip/umetrip.js",
      "script": "https://raw.githubusercontent.com/zZPiglet/Task/master/umetrip/umetrip.js",
      "icons": [
        "https://raw.githubusercontent.com/Orz-3/mini/master/Alpha/umetrip.png",
        "https://raw.githubusercontent.com/Orz-3/mini/master/Color/umetrip.png"]
      }],
  "task": [{
            "config": "0 0 * * * https://raw.githubusercontent.com/zZPiglet/Task/master/umetrip/umetrip.js, tag=航旅纵横, img-url=https://raw.githubusercontent.com/Orz-3/mini/master/Color/umetrip.png, enabled=true",
            "addons": "https://raw.githubusercontent.com/Obsidian99/Profile/master/QuanX/Cookie_Remote.conf, tag=航旅纵横cookie"
        }, 
        {
            "config": "1 0 * * * https://raw.githubusercontent.com/chavyleung/scripts/master/neteasemusic/quanx/neteasemusic.js, tag=网易云音乐, img-url= https://raw.githubusercontent.com/anker1209/icon/main/wyyy.png, enabled=true"
            "addons": "https://raw.githubusercontent.com/Obsidian99/Profile/master/QuanX/Cookie_Remote.conf, tag=网易云音乐cookie"
  }]
}
