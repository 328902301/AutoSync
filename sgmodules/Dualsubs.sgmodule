[MITM]
hostname = %APPEND% www.youtube.com, *.media.dssott.com, *.media.starott.com, atv-ps.amazon.com, atv-ps-fe.primevideo.com, *.hls.row.aiv-cdn.net, cf-timedtext.aux.pv-cdn.net, *-a.akamaihd.net, *.cloudfront.net, *.api.hbo.com, *.hbomaxcdn.com, manifest-dp.hulustream.com, assets.huluim.com, content-discovery.uplynk.com, *-discovery1.uplynk.com, *.cbsaavideo.com, *.cbsivideo.com, *.nflxvideo.net
