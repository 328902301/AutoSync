/*
@Hᴀʙɪᴛ.
解锁夏时国际vip线路
更新日期:2022.09.04
[rewrite_local]
^https?:\/\/hotspot-unlimited\.com\/addressx2\/url script-response-body xsgj.js
[mitm]
hostname = hotspot-unlimited.com
*/

 var body = $response.body;
 var url = $request.url;
 var obj = JSON.parse(body);
const Hᴀʙɪᴛ = '/addressx2/';
if (url.indexOf(Hᴀʙɪᴛ) != -1) {
     obj.hand.vip = 0;
     body = JSON.stringgify(obj);
}

$done({body});
