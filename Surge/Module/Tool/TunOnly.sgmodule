#!name=Tun Only
#!desc=启用 TUN Only 模式，强制 80 端口的域名请求使用 HTTP 引擎处理并打开解密 TCP 流量


[General]
compatibility-mode = 3
# 兼容模式(Only Tun)不能抓包、复写，运行脚本刊，在下面强制加入域名
# > force-http-engine-hosts = %APPEND% *
force-http-engine-hosts = %APPEND% *a, *b, *c, *d, *e, *f, *g, *h, *i, *j, *k, *l, *m, *n, *o, *p, *q, *r, *s, *t, *u, *v, *w, *x, *y, *z

[MITM]
tcp-connection = true
