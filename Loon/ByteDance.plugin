[Rewrite]
^https://aweme-eagle(.*)\.snssdk\.com/aweme/v2/ 302 https://aweme-eagle$1.snssdk.com/aweme/v1/

[Script]
http-request ^https:\/\/.*\.(amemv|musical)\.(com|ly)\/aweme\/v.*\/(feed|post) script-path=https://raw.githubusercontent.com/iEwha/Profiles/master/Script/ByteDance.js, requires-body=true, timeout=10, tag=抖音

http-response ^https:\/\/.*\.(amemv|musical)\.(com|ly)\/aweme\/v.*\/(feed|post) script-path=https://raw.githubusercontent.com/iEwha/Profiles/master/Script/ByteDance.js, requires-body=true, timeout=10, tag=抖音

[MITM]
hostname = *.snssdk.com,*.amemv.com,*.musical.ly