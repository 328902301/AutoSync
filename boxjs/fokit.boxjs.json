{
    "id": "fokit.app.sub",
    "name": "FoKit自用订阅",
    "author": "@FoKit",
    "icon": "https://avatars.githubusercontent.com/u/44187836",
    "repo": "https://github.com/FoKit",
    "apps": [
        {
            "id": "ql",
            "name": "数据同步",
            "author": "@FoKit",
            "descs_html": [
                "用于脑瓜容器、青龙容器和BoxJS之间的数据同步"
            ],
            "icons": [
                "https://raw.githubusercontent.com/FoKit/Quantumult-X/main/images/sync.png",
                "https://raw.githubusercontent.com/FoKit/Quantumult-X/main/images/sync.png"
            ],
            "keys": [
                "@ql.ip_ng",
                "@ql.client_id_ng",
                "@ql.client_secret_ng",
                "@ql.username_ng",
                "@ql.password_ng",
                "@ql.env_ng",
                "@ql.mute_ng",
                "@ql.is_pwd_ng",
                "@ql.ip",
                "@ql.client_id",
                "@ql.client_secret",
                "@ql.username",
                "@ql.password",
                "@ql.env",
                "@ql.mute",
                "@ql.is_pwd"
            ],
            "settings": [
                {
                    "id": "@ql.mute_ng",
                    "name": "静默同步（脑瓜▸BoxJs⁩）",
                    "val": false,
                    "type": "boolean",
                    "desc": "同步时不会显示通知"
                },
                {
                    "id": "@ql.is_pwd_ng",
                    "name": "使用密码登陆",
                    "val": true,
                    "type": "boolean",
                    "desc": "同步时优先使用密码登陆"
                },
                {
                    "id": "@ql.ip_ng",
                    "name": "脑瓜容器登陆地址",
                    "val": "",
                    "type": "text",
                    "placeholder": "ip:port"
                },
                {
                    "id": "@ql.username_ng",
                    "name": "脑瓜容器账号",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.password_ng",
                    "name": "脑瓜容器密码",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.client_id_ng",
                    "name": "脑瓜Client ID",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.client_secret_ng",
                    "name": "脑瓜Client Secret",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.mute",
                    "name": "静默同步（BoxJs⁩▸⁩⁨青龙）",
                    "val": false,
                    "type": "boolean",
                    "desc": "同步时不会显示通知"
                },
                {
                    "id": "@ql.is_pwd",
                    "name": "使用密码登陆青龙容器",
                    "val": false,
                    "type": "boolean",
                    "desc": "同步时优先使用密码登陆"
                },
                {
                    "id": "@ql.ip",
                    "name": "青龙容器登陆地址",
                    "val": "",
                    "type": "text",
                    "placeholder": "ip:port"
                },
                {
                    "id": "@ql.username",
                    "name": "青龙容器账号",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.password",
                    "name": "青龙容器密码",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.client_id",
                    "name": "青龙Client ID",
                    "val": "",
                    "type": "text"
                },
                {
                    "id": "@ql.client_secret",
                    "name": "青龙Client Secret",
                    "val": "",
                    "type": "text"
                }
            ],
            "scripts": [
                {
                    "name": "脑瓜容器同步到BoxJS",
                    "script": "https://raw.githubusercontent.com/FoKit/Quantumult-X/main/scripts/ng_sync_box.js"
                },
                {
                    "name": "BoxJS同步到青龙容器",
                    "script": "https://raw.githubusercontent.com/dompling/Script/master/jd/ql_cookie_sync.js"
                }
            ]
        },
        {
            "id": "wskey",
            "name": "京东WsKey",
            "author": "@FoKit",
            "descs_html": [
                "用于自动抓取京东WsKey"
            ],
            "icons": [
                "https://raw.githubusercontent.com/FoKit/Quantumult-X/main/images/jdjoy.png",
                "https://raw.githubusercontent.com/FoKit/Quantumult-X/main/images/jdjoy.png"
            ],
            "keys": [
                "WSKEY_TG_BOT_TOKEN",
                "WSKEY_TG_USER_ID",
                "wskeyList"
            ],
            "settings": [
                {
                    "id": "WSKEY_TG_BOT_TOKEN",
                    "name": "Telegram机器人Token",
                    "val": "",
                    "type": "text",
                    "desc": "填写Telegram机器人的Token"
                },
                {
                    "id": "WSKEY_TG_USER_ID",
                    "name": "Telegram会话ID",
                    "val": "[\"-1001701121614\"]",
                    "type": "text",
                    "desc": "填写Telegram会话ID，以数组形式保存[\"-1001701121614\"]"
                },
                {
                    "id": "wskeyList",
                    "name": "京东WsKey",
                    "val": "",
                    "type": "text",
                    "desc": "京东WsKey列表"
                }
            ]
        }
    ]
}