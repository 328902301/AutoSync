    {
      "id": "ql",
      "name": "数据同步",
      "author": "@FoKit",
      "descs_html": [
        "用于JD1/JD2和BoxJS之间的数据同步"
      ],
      "icons": [
        "https://raw.githubusercontent.com/FoKit/Scripts/main/images/sync.png",
        "https://raw.githubusercontent.com/FoKit/Scripts/main/images/sync.png"
      ],
      "scripts": [
        {
          "name": "JD1同步到BoxJS",
          "script": "https://raw.githubusercontent.com/FoKit/Scripts/main/scripts/jd1_sync_box.js"
        },
        {
          "name": "JD2同步到BoxJS",
          "script": "https://raw.githubusercontent.com/FoKit/Scripts/main/scripts/jd2_sync_box.js"
        }
      ],
      "keys": [
        "@ql.ip_jd1",
        "@ql.client_id_jd1",
        "@ql.client_secret_jd1",
        "@ql.username_jd1",
        "@ql.password_jd1",
        "@ql.env_jd1",
        "@ql.mute_jd1",
        "@ql.is_pwd_jd1",
        "@ql.ip_jd2",
        "@ql.client_id_jd2",
        "@ql.client_secret_jd2",
        "@ql.username_jd2",
        "@ql.password_jd2",
        "@ql.env_jd2",
        "@ql.mute_jd2",
        "@ql.is_pwd_jd2"
      ],
      "settings": [
        {
          "id": "@ql.mute_jd1",
          "name": "静默同步（JD1 > BoxJs）",
          "val": false,
          "type": "boolean",
          "desc": "同步时不会显示通知"
        },
        {
          "id": "@ql.is_pwd_jd1",
          "name": "使用密码登陆JD1容器",
          "val": false,
          "type": "boolean",
          "desc": "优先使用密码登陆"
        },
        {
          "id": "@ql.ip_jd1",
          "name": "JD1 Domains",
          "val": "",
          "type": "text",
          "placeholder": "ip:port"
        },
        {
          "id": "@ql.username_jd1",
          "name": "JD1 UserName",
          "val": "",
          "type": "text"
        },
        {
          "id": "@ql.password_jd1",
          "name": "JD1 PassWord",
          "val": "",
          "type": "text"
        },
        {
          "id": "@ql.client_id_jd1",
          "name": "JD1 Client ID",
          "val": "",
          "type": "text"
        },
        {
          "id": "@ql.client_secret_jd1",
          "name": "JD1 Client Secret",
          "val": "",
          "type": "text"
        },
        {
          "id": "@ql.mute_jd2",
          "name": "静默同步（JD2 > BoxJs）",
          "val": false,
          "type": "boolean",
          "desc": "同步时不会显示通知"
        },
        {
          "id": "@ql.is_pwd_jd2",
          "name": "使用密码登陆",
          "val": true,
          "type": "boolean",
          "desc": "优先使用密码登陆"
        },
        {
          "id": "@ql.ip_jd2",
          "name": "JD2 Domains",
          "val": "",
          "type": "text",
