#!name= DualSubs
#!desc= (BETA) Dual Subtitiles for Streaming Media Platform

[Script]
DualSubs.YouTube = type=http-request, pattern=^https?:\/\/www\.youtube\.com\/api\/timedtext(.*), requires-body=0, max-size=0, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js
DualSubs.Disney_Plus.hls.m3u8 = type=http-response, pattern=^https?:\/\/(.*)\.media\.(dssott|starott)\.com\/ps01\/disney\/([0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12})\/cbcs-all-(.*)\.m3u8(.*), requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js
# DualSubs.Disney_Plus.webvtt.m3u8 = type=http-response, pattern=^https?:\/\/(.*)\.media\.(dssott|starott)\.com\/ps01\/disney\/([0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12})\/r\/(.*)\.m3u8$, requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js
DualSubs.Disney_Plus.vtt = type=http-response, pattern=^https?:\/\/(.*)\.media\.(dssott|starott)\.com\/ps01\/disney\/([0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12})\/(.*)\.vtt$, requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js
DualSubs.Prime_video.m3u8 = type=http-response, pattern=^https?:\/\/(.*)\.(hls\.row\.aiv-cdn|akamaihd|cloudfront)\.net\/(.*)\/([0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12})\.m3u8$, requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js
DualSubs.Prime_video.vtt = type=http-response, pattern=^https?:\/\/(.*)\.cloudfront\.net\/(.*)\/([0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12})\.vtt$, requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js
DualSubs.HBO_Max.m3u8 = type=http-response, pattern=^https?:\/\/((.*)\.hbomaxcdn|manifests\.v2\.api\.hbo)\.com\/hls\.m3u8(.*), requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js
DualSubs.HBO_Max.vtt = type=http-response, pattern=^https?:\/\/((.*)\.hbomaxcdn|manifests\.v2\.api\.hbo)\.com\/video(.*)\.vtt$, requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js
DualSubs.Hulu.hls.m3u8 = type=http-response, pattern=^https?:\/\/manifest-dp\.hulustream\.com\/hls\/(\d+)\.m3u8\?(.*), requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js
# DualSubs.Hulu.webvtt.m3u8 = type=http-response, pattern=^https?:\/\/manifest-dp\.hulustream\.com\/webvtt\?(.*), requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js
DualSubs.Hulu.vtt = type=http-response, pattern=^https?:\/\/assets\.huluim\.com\/captions_webvtt\/(.+[^blank])\.vtt$, requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js
DualSubs.Paramount_Plus.vtt = type=http-response, pattern=^https?:\/\/(.*)\.cbs(aa|i)video\.com\/(.*)\.vtt(\?m=\d+)?, requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js
DualSubs.Netflix = type=http-response, pattern=^https?:\/\/(.*)\.nflxvideo\.net\/\?o=(\d+)&v=(\d+)&e=(.*), requires-body=1, max-size=0, timeout=30, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.beta.js

[Map Local]
^https?:\/\/raw\.githubusercontent\.com\/DualSubs\/DualSubs\/beta\/database\/DualSubs\.beta\.jsonc data="https://raw.githubusercontent.com/DualSubs/DualSubs/beta/database/DualSubs.beta.jsonc"

[MITM]
hostname = %APPEND% www.youtube.com, *.media.dssott.com, *.media.starott.com, atv-ps.amazon.com, atv-ps-fe.primevideo.com, *.hls.row.aiv-cdn.net, cf-timedtext.aux.pv-cdn.net, *-a.akamaihd.net, *.cloudfront.net, *.api.hbo.com, *.hbomaxcdn.com, manifest-dp.hulustream.com, assets.huluim.com, *.cbsaavideo.com, *.cbsivideo.com, *.nflxvideo.net
