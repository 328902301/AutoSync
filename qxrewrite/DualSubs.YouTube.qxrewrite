#!name= 🍿️ DualSubs for YouTube
#!desc= (0.3.x) YouTube字幕增强及双语模块

#[Script]
# DualSubs.YouTube.Player
^https?:\/\/(www|m)\.youtube\.com\/youtubei\/v1\/player(\?.*)$ url script-response-body https://raw.githubusercontent.com/DualSubs/DualSubs/main/js/DualSubs.YouTube.Player.js
# DualSubs.YouTube.TimedText
^https?:\/\/(www|m)\.youtube\.com\/api\/timedtext(\?.*(format|fmt)=(?!vtt).*)$ url script-response-body https://raw.githubusercontent.com/DualSubs/DualSubs/main/js/DualSubs.YouTube.TimedText.js
# DualSubs.YouTube.vtt
^https?:\/\/(www|m)\.youtube\.com\/api\/timedtext(\?.*(format|fmt)=(vtt).*)$ url script-response-body https://raw.githubusercontent.com/DualSubs/DualSubs/main/js/DualSubs.VTT.js

#[MITM]
hostname = www.youtube.com, m.youtube.com
