      "keys": ["@umetrip.id"],
      "settings": [
        {
          "id": "@umetrip.id",
          "name": "rsid",
          "val": "",
          "type": "text",
          "autoGrow": true,
          "rows": 2,
          "desc": "用户验证信息。若抓包填入请填写 rsid 中「$$」之前的部分。"
