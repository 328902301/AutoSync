name: qx转换
desc: qx重写链接末尾加上qx.stoverride
#author: @小白脸

http:
  script:
    - match: qx.stoverride$
      name: QX转换
      type: request
      require-body: true
      timeout: 30
      max-size: 0
  mitm:
    - "github.com"
    - "raw.githubusercontent.com"

script-providers:
  QX转换:
    url: https://raw.githubusercontent.com/chengkongyiban/stash/main/js/QX_to_Stash.js
    interval: 86400
