#!name=🍟 Cloudflare DDNS
#!desc=(BETA) Auto Update Cloudflare DDNS
#!system=mac

[Rule]
DOMAIN-SUFFIX,my-ip.io,DIRECT

[Script]
Cloudflare DDNS = type=event,network-changed,timeout=60,argument=Token=令牌&zone_id=区域ID&zone_name=区域名称&dns_records_id=记录ID&dns_records_name=记录名称&dns_records_type=记录类型&dns_records_ttl=TTL&dns_records_priority=记录优先级&dns_records_proxied=是否代理,script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Cloudflare_DDNS.beta.js
Cloudflare DDNS = type=cron,cronexp="*/10 * * * *",timeout=60,argument=Token=令牌&zone_id=区域ID&zone_name=区域名称&dns_records_id=记录ID&dns_records_name=记录名称&dns_records_type=记录类型&dns_records_ttl=TTL&dns_records_priority=记录优先级&dns_records_proxied=是否代理,script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Cloudflare_DDNS.beta.js
