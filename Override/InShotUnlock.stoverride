name: InShot
desc: InShot解锁订阅
# 注意：测试中，或不可用
http:
  mitm:
    - "buy.itunes.apple.com"
  script:
    - match: ^https:\/\/buy\.itunes\.apple\.com\/verifyReceipt
      name: InShot
      type: response
      require-body: true
      timeout: 10
script-providers:
  InShot:
    url: https://raw.githubusercontent.com/I-am-R-E/QuantumultX/main/JavaScript/InShot.js
    interval: 86400
